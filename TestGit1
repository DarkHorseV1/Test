package com.faforever;

public class TestGit1
{
	public static void main (String [] args)
	{
		void sayHello()
		{
			System.out.println("----------");
			System.out.println("Hello, Faf!");
			System.out.println("----------");
		}
		void sayGoodbye()
		{
			System.out.println("----------");
			System.out.println("Goodbye, Faf!");
			System.out.println("----------");
		}
			
		sayHello();
		sayGoodbye();
	}	
}
